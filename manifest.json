{
    "name": "Top Sites",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "A new tab that shows your top sites. Inspired by a very old version of Chrome's new tab.",
    "permissions": [
        "topSites"
    ],
    "web_accessible_resources": [
        "src/newtab.js"
    ],
    "chrome_url_overrides": {
        "newtab": "src/newtab.html"
    },
    "icons": {
        "16": "docs/icons/browser16.png",
        "32": "docs/icons/browser32.png",
        "128": "docs/icons/browser128.png"
    }
}